{
	"name": "n8n-nodes-docrouter",
	"version": "0.1.0",
	"description": "n8n nodes for DocRouter.ai",
	"license": "MIT",
	"homepage": "https://github.com/analytiqhub/n8n-nodes-docrouter#readme",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": "Andrei Radulescu-Banu <andrei@analytiqhub.com>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/analytiqhub/n8n-nodes-docrouter.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/DocRouterOrgApi.credentials.js",
			"dist/credentials/DocRouterAccountApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Example/Example.node.js",
			"dist/nodes/DocRouterDocument/DocRouterDocument.node.js",
			"dist/nodes/DocRouterTag/DocRouterTag.node.js",
			"dist/nodes/DocRouterLLM/DocRouterLLM.node.js",
			"dist/nodes/DocRouterPrompt/DocRouterPrompt.node.js",
			"dist/nodes/DocRouterWebhook/DocRouterWebhook.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^20.0.0",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"main": ".prettierrc.js",
	"bugs": {
		"url": "https://github.com/analytiqhub/n8n-nodes-docrouter/issues"
	}
}
